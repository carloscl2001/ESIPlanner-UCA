import pandas as pd
import json

# Ruta del archivo TSV (aj√∫stala si lo tienes en otro sitio)
tsv_path = "C:/Users/Carlos/Downloads/asignaturasInfo.tsv"
json_output_path = "C:/Users/Carlos/Downloads/id_to_horarioID_mapping.json"

# Leer el TSV
df = pd.read_csv(tsv_path, sep="\t")

# Crear el diccionario
mapping = dict(df[['id', 'horarioID']].drop_duplicates().values)

# Guardar el diccionario como JSON
with open(json_output_path, 'w') as f:
    json.dump(mapping, f, indent=4)

print(f"Archivo JSON guardado en: {json_output_path}")
